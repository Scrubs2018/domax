.domek .elem{
    display:block;
    position:absolute;
    top:0;
    left:0;
}

.domek .elem> *{
    opacity: 0.7;
}

.elem a {
    display: table-row;
}

.domek .elem .pointer{
    display:block;
    position:absolute;
    left: 10px;
    top: inherit;
    bottom: -2px;
    pointer-events: none;
}

.domek .elem.right .pointer {
    left: inherit;
    right: 10px;
}

.domek .elem .icon {
    display: table-cell;
    vertical-align: bottom;
    min-width: 95px;
    text-align: center;
}

.elem .icon img {
