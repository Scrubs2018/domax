    width: 1170px;
    height: 731px;
}


@media only screen and (max-width: 1210px) {
    #content .slider-nav .columns.first {
        margin-left: 1rem;
        width: 30.5%;
    }
    #content .domek {
        display: none;
    }
}

.domek {
    position: relative;
    min-width: 1200px;
    height: 760px;
}
.domek .inner {
    width: 1170px;
    height: 590px;
    padding-top: 15px;
    position:relative;
}
.domek .mainimg {
    overflow: visible;
    position:relative;
    top: 7px;
    left: -4px;
}
.domek .mainimg .main {
    height: 730px;
    width:auto;
}

.domek .house,
.domek .gazebo,
.domek .terrace,
.domek .swing,
.domek .pergola,
.domek .children-house,
.domek .sandpit,
.domek .bridge,
.domek .woodshed,
.domek .garage,
.domek .wicket-with-gate,
.domek .carport{
    position: relative;
    top: 12px;
    display:none;
}
