.pointer {
    position: relative;
}

.yellow-line {
    position: relative;
    display: block;
    background: #ffd400;
    width: 2px;
}

.yellow-line.vertical-line {
    height: 2px;
}

.yellow-line.rotate-45 {
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
}

.yellow-line.rotate-45 > .vertical-line {
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
}

.yellow-line.rotate-135 {
    -webkit-transform: rotate(135deg);
    -moz-transform: rotate(135deg);
    -o-transform: rotate(135deg);
    -ms-transform: rotate(135deg);
    transform: rotate(135deg);
}

.yellow-line.rotate-135 > .vertical-line {
    -webkit-transform: rotate(-135deg);
    -moz-transform: rotate(-135deg);
    -o-transform: rotate(-135deg);
    -ms-transform: rotate(-135deg);
    transform: rotate(-135deg);
}

.yellow-line.rotate-225 {
    -webkit-transform: rotate(225deg);
    -moz-transform: rotate(225deg);
    -o-transform: rotate(225deg);
    -ms-transform: rotate(225deg);
    transform: rotate(225deg);
}

.yellow-line.rotate-315 {
    -webkit-transform: rotate(315deg);
    -moz-transform: rotate(315deg);
    -o-transform: rotate(315deg);
    -ms-transform: rotate(315deg);
    transform: rotate(315deg);
}
.yellow-line.rotate-315 > .horizontal-line,
.yellow-line.rotate-315 > .vertical-line {
    -webkit-transform: rotate(225deg);
    -moz-transform: rotate(225deg);
    -o-transform: rotate(225deg);
    -ms-transform: rotate(225deg);
    transform: rotate(225deg);
}

.yellow-line .yellow-circle {
    display: block;
    width: 14px;
    height: 14px;
    border-radius: 50%;
    border: 3px solid #ffd400;
    position: absolute;
    bottom: -14px;
    left: -6px;
}

.yellow-line.horizontal .yellow-circle {

}

.yellow-line.vertical-line .yellow-circle {
    left: inherit;
    right: -13px;
    bottom: -6px;
}

.right .yellow-line.vertical-line .yellow-circle {
    left: -13px;
    right: inherit;
    bottom: -6px;
}

.yellow-line.vertical-line .rotate > .yellow-circle {
    bottom: -14px;
    left: -6px;
    right: inherit;
}

