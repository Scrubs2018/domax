.domek .prev-button{
    position:absolute;
    top:0;
    right:0;
    cursor:pointer;
    display:none;
}

.domek .element{
    display:block;
    position:absolute;
    opacity: 0.75;
    top:0;
    left:0;
    height:1px;
    width:1px;
}

.domek .element .pointer{
    display:block;
    position:absolute;
    top:0;
    left:0;
    pointer-events: none;
}

.domek .element .icon{
    display:block;
    position:absolute;
    top:0;
    left:0;
}

.domek .element .pointer img{
    max-width: none;
}

.domek .element .text{
    display:block;
    position: relative;
    color: #2e2e2e;
    font: 16px titillium-bold;
    text-transform: uppercase;
    cursor:pointer;
}

.domek .element .text:hover{
    opacity:1;
}

#maparea area{
    cursor:pointer;
}

.domek .mainimg .overlay{
    display:block;
    height:100%;
    width:100%;
    position: absolute;
    top: 0;
    left:0;
    pointer-events: none;
}

.domek .mainimg .main{
    width:1170px;
    height:731px;
}

.domek .mainimg .overlay img{
    pointer-events: none;
    display:none;
}

.domek .mainimg .miniature {
    width: 80px;
    height: 50px;
    border-radius: 10px;
    cursor: pointer;
    border: 1px #d8d8d8 solid;
    position: absolute;
}

.domek .mainimg .miniature:hover,
.domek .mainimg .miniature.hovered {
    border: 1px rgb(255, 213, 3) solid;
}

.inner>div>img {
    display: block;
    margin-left: auto;
    margin-right: auto;
    border-radius: 10px;
}
.active_space>div {
    width: 80px;
    display: inline-block;
    border-radius: 10px;
    cursor: pointer;
    z-index: 5;
}
.active_space>div:hover {
    opacity: 1;
}
.active_space .mainimg_slide {
    vertical-align: middle;
    width: 150px;
    border-radius: 0px;
}
.active_space .house_slide {
    background: url(../img/dmx/house.jpg);
}
.active_space .gazebo_slide {
    background: url(../img/dmx/gazebo.jpg);
}
.active_space .terrace_slide {
    background: url(../img/dmx/terrace.jpg);
}
.active_space .pergola_slide {
    background: url(../img/dmx/perola.jpg);
}
.active_space .children-house_slide {
